{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@MelosFlow/config": [
        "libs/config/src"
      ],
      "@MelosFlow/config/*": [
        "libs/config/src/*"
      ],
      "@MelosFlow/common": [
        "libs/common/src"
      ],
      "@MelosFlow/common/*": [
        "libs/common/src/*"
      ],
      "@MelosFlow/db": [
        "libs/db/src"
      ],
      "@MelosFlow/db/*": [
        "libs/db/src/*"
      ],
      "@MelosFlow/type": [
        "libs/type/src"
      ],
      "@MelosFlow/type/*": [
        "libs/type/src/*"
      ]
    }
  }
}